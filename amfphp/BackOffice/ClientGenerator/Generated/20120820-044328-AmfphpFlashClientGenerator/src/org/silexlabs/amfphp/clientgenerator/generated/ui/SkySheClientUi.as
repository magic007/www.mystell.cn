package org.silexlabs.amfphp.clientgenerator.generated.ui{	import fla.Label;	import fla.Button;	import fla.TextInput;	import fla.TextResultDisplay;		import flash.display.MovieClip;	import flash.display.Sprite;	import flash.events.MouseEvent;	import flash.net.NetConnection;	import flash.text.TextField;	import flash.text.TextFieldAutoSize;	import flash.text.TextFieldType;		import org.silexlabs.amfphp.clientgenerator.NetConnectionSingleton;	import org.silexlabs.amfphp.clientgenerator.generated.ObjectUtil;	import org.silexlabs.amfphp.clientgenerator.generated.service.SkySheClient;	import org.silexlabs.amfphp.clientgenerator.ui.elements.IDataIoGui;	import org.silexlabs.amfphp.clientgenerator.ui.elements.ILabel;		public class SkySheClientUi extends MovieClip	{		/**		 * layout constants		 * */		private static const PADDING:int = 20;		private static const METHOD_UI_HEIGHT:int = 40;		private static const METHOD_INPUT_WIDTH:int = 120;		private static const CALL_BUTTON_X:int = 400;				private var _service:SkySheClient;						//sayHello inputs 		public var sayHelloLabel:ILabel;		public var sayHello_callButton:ILabel;				public var resultDisplay:IDataIoGui;		public function SkySheClientUi()		{			super();			var xLayoutMultiplier:int = 0;			var yLayoutMultiplier:int = 0;			var maxNumMethodInputs:int = 0;						_service = new SkySheClient(NetConnectionSingleton.getNetConnection());			graphics.beginFill(0, 0.3);						//sayHello ui			xLayoutMultiplier = 0;			if(!sayHelloLabel){				sayHelloLabel = new Label();				sayHelloLabel.label = 'sayHello method';				sayHelloLabel.displayObject.x = PADDING;				sayHelloLabel.displayObject.y = PADDING + METHOD_UI_HEIGHT * yLayoutMultiplier;				addChild(sayHelloLabel.displayObject);							}												if(!sayHello_callButton){				sayHello_callButton = new Button();				sayHello_callButton.label = "call";				sayHello_callButton.displayObject.x = PADDING + METHOD_INPUT_WIDTH * xLayoutMultiplier;				sayHello_callButton.displayObject.y = PADDING + sayHelloLabel.displayObject.height  + METHOD_UI_HEIGHT * yLayoutMultiplier;				addChild(sayHello_callButton.displayObject);			}			sayHello_callButton.addEventListener(MouseEvent.CLICK, sayHello_callButtonClickHandler);			yLayoutMultiplier++;						if(!resultDisplay){				resultDisplay = new TextResultDisplay();				resultDisplay.displayObject.y = 2 * PADDING + METHOD_UI_HEIGHT * yLayoutMultiplier;				addChild(resultDisplay.displayObject);			}					}	  								private function sayHello_callButtonClickHandler(event:MouseEvent):void{			_service.sayHello().setResultHandler(sayHelloResultHandler).setErrorHandler(errorHandler); 		}		private function sayHelloResultHandler(obj:Object):void{			resultDisplay.data = "result : \n" + obj + '\n' + ObjectUtil.deepObjectToString(obj) + "\n";		}						private function errorHandler(obj:Object):void{			resultDisplay.data = "error :  \n" +  ObjectUtil.deepObjectToString(obj) + "\n";		}	}}