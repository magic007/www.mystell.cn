package{		import fla.Button;		import flash.display.MovieClip;	import flash.display.Sprite;	import flash.display.StageScaleMode;	import flash.events.MouseEvent;	import flash.net.NetConnection;		import org.silexlabs.amfphp.clientgenerator.generated.ui.ExampleServiceClientUi;	import org.silexlabs.amfphp.clientgenerator.generated.ui.SkySheClientUi;	import org.silexlabs.amfphp.clientgenerator.ui.elements.ILabel;	import org.silexlabs.amfphp.clientgenerator.ui.elements.Link;		public class AmfphpFlashClient extends MovieClip	{		private static const PADDING:int = 20;		private static const HEADER_HEIGHT:int = 100;		private var _serviceUi:Sprite;		private var _maxServiceUiWidth:Number;				public var showExampleServiceUiBtn:ILabel;				public var showSkySheUiBtn:ILabel;				public function AmfphpFlashClient()		{			var exchangeLink:Link = new Link();						var layoutMultiplier:int = 0;			_maxServiceUiWidth = 0;									var showExampleServiceUiBtn:Button = new Button();			showExampleServiceUiBtn.displayObject.x = PADDING;			showExampleServiceUiBtn.displayObject.y = HEADER_HEIGHT + (showExampleServiceUiBtn.displayObject.height + PADDING) * layoutMultiplier;			showExampleServiceUiBtn.label = 'ExampleService';			showExampleServiceUiBtn.addEventListener(MouseEvent.CLICK, showExampleServiceUiBtnClickHandler);			addChild(showExampleServiceUiBtn);			layoutMultiplier++;			if(showExampleServiceUiBtn.width > _maxServiceUiWidth){				_maxServiceUiWidth = showExampleServiceUiBtn.width; 			}						var showSkySheUiBtn:Button = new Button();			showSkySheUiBtn.displayObject.x = PADDING;			showSkySheUiBtn.displayObject.y = HEADER_HEIGHT + (showSkySheUiBtn.displayObject.height + PADDING) * layoutMultiplier;			showSkySheUiBtn.label = 'SkyShe';			showSkySheUiBtn.addEventListener(MouseEvent.CLICK, showSkySheUiBtnClickHandler);			addChild(showSkySheUiBtn);			layoutMultiplier++;			if(showSkySheUiBtn.width > _maxServiceUiWidth){				_maxServiceUiWidth = showSkySheUiBtn.width; 			}					}				private function showServiceUi(newUi:Sprite):void{			if(_serviceUi){				removeChild(_serviceUi);			}			_serviceUi = newUi;			_serviceUi.x = _maxServiceUiWidth + 2 * PADDING;			_serviceUi.y = HEADER_HEIGHT;			addChild(_serviceUi);		}								private function showExampleServiceUiBtnClickHandler(event:MouseEvent):void{			showServiceUi(new ExampleServiceClientUi());			}						private function showSkySheUiBtnClickHandler(event:MouseEvent):void{			showServiceUi(new SkySheClientUi());			}					}}